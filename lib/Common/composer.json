{
	"name":              "dreamfactory/dfe-common",
	"description":       "DreamFactory Enterprise(tm) Common Components",
	"homepage":          "https://www.dreamfactory.com/",
	"keywords":          [
		"dreamfactory",
		"fabric",
		"utility",
		"queue",
		"common"
	],
	"license":           ["proprietary"],
	"type":              "library",
	"authors":           [
		{
			"name":  "Jerry Ablan",
			"email": "jerryablan@dreamfactory.com"
		}
	],
	"repositories":      [
		{
			"type": "vcs",
			"url":  "git@github.com:dreamfactorysoftware/fabric-common.git"
		},
		{
			"type": "vcs",
			"url":  "git@github.com:dreamfactorysoftware/fabric-db.git"
		},
		{
			"type": "vcs",
			"url":  "git@github.com:dreamfactorysoftware/fabric-queue-models.git"
		},
		{
			"type": "vcs",
			"url":  "git@github.com:dreamfactorysoftware/fabric-couchdb.git"
		}
	],
	"minimum-stability": "dev",
	"require":           {
		"wpb/string-blade-compiler":   "2.*@dev",
		"league/flysystem-sftp":       "*",
		"league/flysystem-ziparchive": "*",
		"league/flysystem-dropbox":    "*",
		"dreamfactory/fabric-db":      "dev-develop as dev-master"
	},
	"require-dev":       {
		"dreamfactory/fabric-audit":  "dev-develop as dev-master",
		"dreamfactory/fabric-common": "dev-develop as dev-master",
		"laravel/framework":          "5.0.*",
		"phpunit/phpunit":            "~4.0",
		"phpspec/phpspec":            "~2.1"
	},
	"extra":             {
		"branch-alias": {
			"dev-develop": "0.1.x-dev"
		}
	},
	"autoload":          {
		"psr-4": {
			"DreamFactory\\Enterprise\\Common\\": "src/"
		}
	},
	"config":            {
		"preferred-install": "dist"
	}
}
