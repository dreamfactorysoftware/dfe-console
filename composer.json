{
    "name":              "dreamfactory/dfe-console",
    "description":       "DreamFactory Enterprise(tm) Administrative Console",
    "homepage":          "https://www.dreamfactory.com/",
    "keywords":          [
        "dreamfactory", "fabric", "utility", "cli"
    ],
    "license":           ["proprietary"],
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories":      [
        {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/dfe-common.git"
        }, {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/dfe-services.git"
        }, {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/rave-common.git"
        }, {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/fabric-common.git"
        }, {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/fabric-db.git"
        }, {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/fabric-queue.git"
        }, {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/fabric-queue-models.git"
        }, {
            "type": "vcs",
            "url":  "git@github.com:dreamfactorysoftware/fabric-couchdb.git"
        }
    ],
    "type":              "project",
    "require":           {
        "laravel/framework":              "5.0.*",
        "doctrine/couchdb-odm":           "*",
        "ruflin/elastica":                "*",
        "dreamfactory/dfe-services":      "dev-develop as dev-master",
        "dreamfactory/php-utils":         "*",
        "dreamfactory/rave-common":       "dev-develop as dev-master",
        "dreamfactory/fabric-api-common": "dev-develop as dev-master",
        "dreamfactory/fabric-audit":      "dev-develop as dev-master",
        "dreamfactory/console-tools":     "dev-develop as dev-master",
        "dreamfactory/fabric-queue":      "dev-develop as dev-master"
    },
    "require-dev":       {
        "phpunit/phpunit":             "~4.0",
        "phpspec/phpspec":             "~2.1",
        "doctrine/dbal":               "~2.3",
        "barryvdh/laravel-ide-helper": "~2.0",
        "way/generators":              "~3.0"
    },
    "autoload":          {
        "classmap": [
            "database"
        ], "psr-4": {
            "DreamFactory\\Enterprise\\Console\\": "app/"
        }
    },
    "autoload-dev":      {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts":           {
        "post-install-cmd":           [
            "php artisan clear-compiled",
            "php artisan optimize"
        ], "post-update-cmd":         [
            "php artisan clear-compiled",
            "php artisan optimize"
        ], "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config":            {
        "preferred-install": "dist"
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
